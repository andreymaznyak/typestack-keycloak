  "bugs": {
    "url": "https://github.com/andreymaznyak/typestack-keycloak/issues"
  },
  "homepage":
    "https://github.com/andreymaznyak/typestack-keycloak/issues#README",
  "devDependencies": {
    "@types/node": "^9.3.0",
    "cpx": "^1.5.0",
    "nock": "^9.1.6",
    "routing-controllers": "^0.7.6",
    "tap": "^11.0.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "get-keycloak-public-key-with-agent": "1.0.5",
    "jsonwebtoken": "^8.1.0"
