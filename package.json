{
  "name": "typestack-keycloak",
  "version": "1.0.3",
  "description":
    "Repository contains routing controllers middlewares for keycloak integration",
  "main": "index.js",
  "scripts": {
    "test": "npm run build && tap tests/*.spec.js",
    "build": "npm run tsc",
    "cpx": "cpx",
